		<property name="backGroup" column="back_group"  type="java.lang.String"/>
		<property name="pointOrg" column="point_org"  type="java.lang.String"/>
		
		<property name="reconDate" column="recon_date"  type="java.lang.String"/>
		<property name="virtualTradeType" column="virtual_trade_type"  type="java.lang.String"/>
		<property name="importDate" column="import_date"  type="java.lang.String"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

