<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
 	<class name="com.froad.recon.sys.model.Platform" table="platform" >

        <id name="platformNo" column="platform_no" type="java.lang.String">
            <generator class="assigned" />
        </id>
		
		
		<property name="platformName" column="platform_name"  type="java.lang.String"/>
		<property name="status" column="status"  type="java.lang.String"/>
		<property name="createTime" column="create_time"  type="java.util.Date"/>
		<property name="updateTime" column="update_time"  type="java.util.Date"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

